<div id="mauticform_wrapper_qualification" class="mauticform_wrapper">
    <form autocomplete="false" role="form" method="post" action="https://content.goabroadpass.com/form/submit?formId=1" id="mauticform_qualification" data-mautic-form="qualification" enctype="multipart/form-data">
        <div class="mauticform-error" id="mauticform_qualification_error"></div>
        <div class="mauticform-message" id="mauticform_qualification_message"></div>
        <div class="mauticform-innerform">
            
          <div class="mauticform-page-wrapper mauticform-page-1" data-mautic-form-page="1">

            <div id="mauticform_qualification_email" data-validate="email" data-validation-type="email" class="mauticform-row mauticform-email mauticform-field-1 mauticform-required">
                <label id="mauticform_label_qualification_email" for="mauticform_input_qualification_email" class="mauticform-label">Email</label>
                <input id="mauticform_input_qualification_email" name="mauticform[email]" value="" class="mauticform-input" type="email">
                <span class="mauticform-errormsg" style="display: none;">Required</span>
            </div>

            <div id="mauticform_qualification_born" data-validate="born" data-validation-type="date" class="mauticform-row mauticform-date mauticform-field-2 mauticform-required">
                <label id="mauticform_label_qualification_born" for="mauticform_input_qualification_born" class="mauticform-label">Born</label>
                <input id="mauticform_input_qualification_born" name="mauticform[born]" value="" class="mauticform-input" type="date">
                <span class="mauticform-errormsg" style="display: none;">Required</span>
            </div>

            <div id="mauticform_qualification_eu" data-validate="eu" data-validation-type="select" class="mauticform-row mauticform-select mauticform-field-3 mauticform-required">
                <label id="mauticform_label_qualification_eu" for="mauticform_input_qualification_eu" class="mauticform-label">EU</label>
                <select id="mauticform_input_qualification_eu" name="mauticform[eu]" value="" class="mauticform-selectbox">                    <option value=""></option>                    <option value="eu">EU</option>                    <option value="noneu">non EU</option>
                </select>
                <span class="mauticform-errormsg" style="display: none;">This is required.</span>
            </div>

            <div id="mauticform_qualification_submit" class="mauticform-row mauticform-button-wrapper mauticform-field-4">
                <button type="submit" name="mauticform[submit]" id="mauticform_input_qualification_submit" value="" class="mauticform-button btn btn-default">Submit</button>
            </div>
            </div>
        </div>

        <input type="hidden" name="mauticform[formId]" id="mauticform_qualification_id" value="1">
        <input type="hidden" name="mauticform[return]" id="mauticform_qualification_return" value="">
        <input type="hidden" name="mauticform[formName]" id="mauticform_qualification_name" value="qualification">

        </form>


	
